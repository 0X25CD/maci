{
  "name": "maci",
  "version": "0.0.1a",
  "description": "Minimal Anti-Collusion Infrastructure",
  "repository": "git@github.com:kendricktan/maci.git",
  "author": "Kendrick Tan <kendricktan0814@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "flow-node app/index.js",
    "dev": "nodemon \"flow-node app/index.js\"",
    "circuit:compile": "mkdir -p app/circuits && circom circuits/circuit.circom -o app/circuits/circuit.json",
    "circuit:setup": "cd app/circuits && snarkjs setup",
    "circuit:generateverifier": "mkdir -p contracts && cd app/circuits && snarkjs generateverifier && mv verifier.sol ../../contracts/Verifier.sol",
    "gg": "flow-node --experimental-worker app/utils/crypto.js",
    "gg2": "node app/utils/generate_circuit_input.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "circom": "0.0.31",
    "circomlib": "0.0.6",
    "express": "^4.17.1",
    "flow-bin": "^0.107.0",
    "flow-remove-types": "^2.107.0",
    "snarkjs": "^0.1.19",
    "wasmbuilder": "^0.0.8",
    "websnark": "^0.0.5"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-node": "^10.0.0",
    "nodemon": "^1.19.2"
  }
}
